<li id="<%= feed_item.id %>">
  <span class="memo-title"><%= feed_item.info %></span>
  <span class="memo-delete"><%= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: "delete " + feed_item.link %>
  </span><br>
  <span class="memo-link"><a target="_blank" href=<%= feed_item.link %>><%= feed_item.link %></a></span><br>
  
  <span class="timestamp">
    <!-- Posted <%= time_ago_in_words(feed_item.created_at) %> ago. -->
  </span>
</li>